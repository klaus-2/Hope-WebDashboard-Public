<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./partials/_seo.ejs') %>
  <title>Servers - Hope</title>
  <style>
    body {
      background-color: #0e1015;
      font-family: Arial, Helvetica, sans-serif;
      color: #bbbbbb;
      display: flex;
      align-items: center;
      justify-content: center;
      flex-direction: column;
    }

    h3 {
      background-color: rgb(33, 33, 33);
      border-radius: 15px;
      padding: 10px 10px;
      font-size: 10px;
      color: rgb(148, 148, 148);
      margin-top: 1.5rem;
    }
  </style>
</head>

<body>
  <%- include("./partials/_navbar-servers.ejs") %>
  <h1 style="padding-top: 100px;text-align: center;font-size: 3em;font-weight: 700;line-height: 2em;font-family: system-ui;letter-spacing: -.009em;color: #e2e4e9;" name="translate" caption="servers_title">SELECT A SERVER</h1>

  <% if (userExists.message == "Unknown Member") { %>
  <center>
    <div style="width: 512px;
       padding: 16px;
       margin: 8px; background-color: hsl(225deg 14% 11%); border-radius: 10px;">
      <h2 style="color: #ff5766; margin-top: 0;margin-bottom: 8px;font-size: 18px;font-family: 'Poppins', sans-serif;"><span name="translate" caption="servers_notice1">Hey</span> <%= user.username%>!</h2>
      <p style="margin: 0;font-size: 14px;font-family: 'Open Sans', sans-serif;color: #555;"><span name="translate" caption="servers_notice2">Looks like you aren't in Hope's Support Server.</span> <a style="color: #ff5766;" href="/support" name="translate" caption="servers_notice3">Join Here!</a></p>
    </div>
  </center>
  <% } %>

  <div id="user_guilds" class="fuck">
    <% user.guilds.forEach(guild=> { 
      const permsOnGuild = new PermissionsBitField(BigInt(guild.permissions));
      if (!permsOnGuild.has(PermissionsBitField.Flags.ManageGuild)) return;  %>

    <div class="xd">
      <div class="card" style="width: 304px;
      height: 340px;">
        <img class="header" src="https://preview.redd.it/4zh2hgl46cp51.png?width=3325&format=png&auto=webp&s=b9123bff12e1d5b86248d27a059104b4c92e05b5" />
        <span class="logo">
          <img src="<%- guild.icon ? `https://cdn.discordapp.com/icons/${guild.id}/${guild.icon}` : `https://ui-avatars.com/api/?name=${guild.name}&background=5865f2&color=fff` %>" />
        </span>
        <% if (guild.owner === true) { %>
        <span style="margin-left: 225px;">üëë</span>
        <% } %>
        <div class="content">
          <p class="title"><%= guild.name %></p>
          <p class="description">
            ‚ÄÅ
          </p>
          <hr class="ql">
          <center><a href="/dashboard/<%- guild.id %>/home" style="text-decoration: none;"><button class="button dashboard" style="font-family: system-ui;" name="translate" caption="servers_button"> DASHBOARD </button></a></center>
          <!-- <hr class="ql">
          <div class="stats">
            <span class="online"> Online</span>
            <span class="members"> Members</span>
          </div> -->
        </div>
      </div>
    </div>
    <% }); %>
  </div>
  <!-- <center><a href="<%= domain %>/logout?to=<%= domain %>/login?reload=true" class="cta" style="text-decoration: none; user-select: none;">Reload servers</a></center> -->
